# --- Domain / TLS ---
APP_DOMAIN=noricare.app
ACME_EMAIL=you@example.com

# --- Backend ---
GEMINI_API_KEY=replace-me
GEMINI_MODEL_NAME=gemini-3-flash-preview

# Strong secret for JWT (generate a long random string)
JWT_SECRET_KEY=replace-with-64+chars-random
ACCESS_TOKEN_EXPIRE_MINUTES=43200

# CORS: allow your deployed web origin(s)
# Comma-separated or JSON array
BACKEND_CORS_ORIGINS=https://noricare.app

# --- Postgres ---
POSTGRES_DB=personalhealth
POSTGRES_USER=personalhealth
POSTGRES_PASSWORD=replace-with-strong-password

# Optional: override backend display name
PROJECT_NAME=NoriCare
